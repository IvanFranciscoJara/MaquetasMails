<mjml>
  <mj-head>
    <mj-breakpoint width="768px" />
    <mj-attributes>
      <mj-all padding="0px" font-size="16px"></mj-all>
      <mj-class name="preheader" color="#000000" font-size="11px" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding="0px"></mj-class>
    </mj-attributes>
    <mj-style>
      @media only screen and (max-width:480px) {
        .img-responsive {
          height: auto !important;
          width: 100% !important;
        }
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#ffffff" width="600px">
    <mj-body>
      <mj-section background-color="#ffffff" padding="0 0">
        <mj-column width="100%">
          <mj-image src="./header.gif" alt="Mi Mundo Rimac" title="Mi Mundo Rimac" padding="0 0" href="https://personas.rimac.com/micuenta/login?mcid=emailing:mail:11_2020_zona-privada_conversion:bbdd_mundo_rimac:inicia-sesion:boton" />
        </mj-column>
      </mj-section>
      <mj-section background-color="#ffffff" padding="20px 28px 20px 28px">
        <mj-column>
            <mj-text align="left" color="#494F66" font-size="32px" font-family="Arial, sans-serif" font-weight="bold" padding="0px 20px" line-height="40px" padding-bottom="32px">
              Estas funciones te encantarán              
            </mj-text>
        </mj-column>
      </mj-section>
      <mj-section background-color="#ffffff" padding="0px 28px 32px 28px" css-class="cards">
