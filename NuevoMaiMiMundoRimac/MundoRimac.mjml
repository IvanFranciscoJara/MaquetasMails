          <mj-text align="left" color="#494F66" font-size="24px" font-weight="bold" font-family="Arial, sans-serif" vertical-align="middle" padding="0 28px 12px 28px">
            Mis seguros
          </mj-text>
          <mj-text align="left" color="#676F8F" font-size="16px" line-height="20px" font-family="Arial, sans-serif" vertical-align="top" padding="0 28px 16px 28px">
            Revisa el detalle de tus <br /> seguros al toque.
          </mj-text>
          <mj-button align="left" background-color="#EF3340" font-weight="bold" border-radius="6px" font-size="10px" color="#ffffff" padding="0 28px" inner-padding="8px 16px" href="https://personas.rimac.com/?resource=/micuenta/mis-servicios&mcid=emailing:mail:11_2020_zona-privada_conversion:bbdd_mundo_rimac:mis-seguros:boton">
            <span style="color: #ffffff; text-decoration:none;">VER SEGUROS</span>
          </mj-button>
          <mj-table cellpadding="0" cellspacing="0" width="100%" align="center">
            <tr>
              <td>
                <table width="100%" cellspacing="0" cellpadding="0" border="0">
                  <tr>
                    <td>
                      <img src="./item1.png" width="252" height="147" alt="Mis seguros rimac" style="margin:0; padding:0; border:none; display:block;" border="0" class="img-responsive" />
                    </td>
                  </tr>
                </table>
              </td>
            </tr>
          </mj-table>
        </mj-column>
        <mj-column width="16px">
          <mj-spacer height="32px" />
        </mj-column>
        <mj-column width="252px" border-radius="12px" padding-top="32px" background-color="#F7F8FC" css-class="card">
          <mj-text align="left" color="#494F66" font-size="24px" font-weight="bold" font-family="Arial, sans-serif" vertical-align="middle" padding="0 28px 12px 28px">
            Mis pagos
          </mj-text>
          <mj-text align="left" color="#676F8F" font-size="16px" line-height="20px" font-family="Arial, sans-serif" vertical-align="top" padding="0 28px 16px 28px">
